"findlcm" (
  over over findgcd / *
) proc

"Lowest common multiple of 5 and 10: " $
5 10 findlcm .

"Lowest common multiple of 6 and 9: " $
6 9 findlcm .

"Lowest common multiple of 7 and 8: " $
7 8 findlcm .
